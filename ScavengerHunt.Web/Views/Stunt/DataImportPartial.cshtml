@model object

@using (Html.BeginForm("DataImport", "Stunt"))
{
    @Html.AntiForgeryToken()

    <div class="form" role="form">
        <div class="mb-3">
            @Html.TextArea("data", new { @class = "form-control", @style = "height: 300px;" })
        </div>

        <input type="submit" value="Import" class="btn btn-primary" />
    </div>
}